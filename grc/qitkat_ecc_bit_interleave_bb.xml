<?xml version="1.0"?>
<block>
  <name>Bit Interleave</name>
  <key>qitkat_ecc_bit_interleave_bb</key>
  <category>qitkat.ecc</category>
  <import>import qitkat</import>
  <make>qitkat.ecc_bit_interleave_bb($block)</make>
  <param>
    <name>Block</name>
    <key>block</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
  <doc>
	Interleave bits in a byte stream.
  </doc>
</block>
