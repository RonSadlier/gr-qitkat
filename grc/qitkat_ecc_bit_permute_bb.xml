<?xml version="1.0"?>
<block>
	<name>Bit Permute</name>
	<key>qitkat_ecc_bit_permute_bb</key>
	<category>qitkat.ecc</category>
	<import>import qitkat</import>
	<make>qitkat.ecc_bit_permute_bb($permutation)</make>
	<param>
		<name>Permutation</name>
		<key>permutation</key>
		<type>int_vector</type>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
	<doc>
		Interleave bits in a byte stream.
	</doc>
</block>
