<?xml version="1.0"?>
<block>
	<name>Get Hardware Data</name>
	<key>qitkat_get_hardware_data</key>
	<category>qitkat</category>
	<import>import qitkat</import>
	<make>qitkat.get_hardware_data($address, $port, $requested)</make>
	<param>
		<name>Address</name>
		<key>address</key>
		<type>string</type>
	</param>
	<param>
		<name>Request Amount</name>
		<key>requested</key>
		<type>int</type>
	</param>
	<param>
		<name>Port</name>
		<key>port</key>
		<type>int</type>
	</param>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
</block>
